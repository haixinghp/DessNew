#include "PlatformFunc.h"
#include "SystemInit.h"
#include "Scheduler.h" 
#include "Public.h"

/*********************************************************************************************************************
* Function Name :  PlatformSystemInit()
* Description   :  52832 平台系统初始化
* Para          :  void
* Return        :  void
*********************************************************************************************************************/
void PlatformSystemInit(void)
{
    SystemConfigInit(); 
    return;
}


/*********************************************************************************************************************
* Function Name :  PlatformLoopExecute()
* Description   :  52832 平台系统调度函数
* Para          :  void
* Return        :  void
*********************************************************************************************************************/
void PlatformLoopExecute(void)
{
	while(1)
	{
		App_Scheduler_Process();	
	}
}




















